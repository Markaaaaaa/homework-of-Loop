using System;

class Program {
  public static void Main (string[] args) {
   
    int odd=0;
    int even=0;
    Console.WriteLine("Enter your number");
    int num;
    for(int i=1; i<=100; i++)
    {
      num =Convert.ToInt32(Console.ReadLine());
      if(num % 2 ==0)
      {
        even+=1;
      }
      else if(num%2==1)
      {
        odd+=1;
      }
    }
    Console.WriteLine("There are {0} even numbers and {1} odd numbers", even, odd);
  }
}
